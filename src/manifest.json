{
    "manifest_version": 2,
    "name": "__MSG_extName__",
    "version": "2.4.3",
    "description": "__MSG_extDescription__",
    "default_locale": "en",
    "permissions": [
        "contextMenus",
        "tabs",
        "<all_urls>"
    ],
    "background": {
        "scripts": [ "script.js" ],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": [ "<all_urls>" ],
            "js": [ "content.js" ],
            "run_at": "document_start",
            "all_frames": true
        }
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}

